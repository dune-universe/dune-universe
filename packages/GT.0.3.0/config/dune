(executable
  (name discover)
  (modules Discover)
  (libraries str dune-configurator)
)

(rule
  (targets camlp5-flags.cfg )
  (action (run ./discover.exe -all-flags))
)

(rule
  (targets dune.tests regression.t)
  (alias discover-tests)
  ;(deps tests.dune.tpl (source_tree ../regression))
  (mode (promote-into ../regression))
  (action (run ./discover.exe -tests -tests-dir %{project_root}/../../regression))
)
